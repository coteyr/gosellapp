<!DOCTYPE html>
<html>
    <%= render 'layouts/head' %>

  <body>
    <%= render 'layouts/navbar' %>
    <% if notice %>
      <p class="alert alert-success"><%= notice %></p>
    <% end %>
    <% if alert %>
      <p class="alert alert-danger"><%= alert %></p>
    <% end %>
    <%= yield %>
    <%= render "layouts/footer" %>
  </body>
</html>
