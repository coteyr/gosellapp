<% @prospect.notes.each do |n| %>
  <% if !n.new_record? %>
    <% if n.detail.present? %> 
      <div class="row">
        <div class="col-xs-12">
          Note: <%= n.created_at.strftime("%m/%d/%y") %> - <%= n.detail %>
        </div>
      </div>
    <% end %>
  <% end %>
<% end %>
