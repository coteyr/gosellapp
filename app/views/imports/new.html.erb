<%= row do %>
<%= render partial: 'layouts/errors', locals: {objekt: @import} %>
  <%= well 'Upload CSV' do %>
    <%= form_for @import, multipart: true, class: 'form-inline' do |f| %>
      <%= f.hidden_field :user_id, value: current_user.id %>
      <%= f.hidden_field :group_id, value: current_user.group_id %>
      <%= f.file_field :data %>
      <%= f.hidden_field :data_cache %>
      <%= panel_footer do %>
        <%= f.submit 'Submit', class: 'pull-right' %>
      <% end %>
    <% end %>
  <% end %>
<% end %>
